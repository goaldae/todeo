    "axios": "^0.19.2",
    "babel-loader": "^8.0.0-beta.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.2",
    "scss-loader": "0.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && set WEBPACK_ENV=development&&webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "xcopy .\\src\\static .\\build\\static /s /y && xcopy .\\src\\views .\\build\\views /s /y",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rd /s /q build",
    "start": "node build/init.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^6.8.0",
    "jest": "^26.3.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}
